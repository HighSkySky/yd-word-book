{
  "name": "yd-word-book",
  "version": "0.0.2",
  "main": "index.js",
  "author": "sanwu <highskysky97@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HighSkySky/yd-word-book"
  },
  "bin": {
    "yd": "dist/command/yd.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "types:check": "tsc --noEmit",
    "format": "prettier --write 'src/**/*.ts'",
    "publish": "rm -rf ./dist && tsc && npm publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run types:check && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run format",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "cheerio": "^1.0.0-rc.3",
    "colors": "^1.4.0",
    "commander": "^4.0.1",
    "cookie": "^0.4.0",
    "is-chinese": "^1.2.9",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.14",
    "@types/cookie": "^0.3.3",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^12.12.8",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "typescript": "^3.7.2"
  },
  "files": [
    "README.md",
    "dist"
  ]
}
